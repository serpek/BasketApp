{
  "name": "basket-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build:prod": "npm run env -s && ng build --prod",
    "build:test": "npm run env -s && ng build --configuration=test",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "start:dev": "concurrently --kill-others \"npm start\" \"npm run mock:server\"",
    "mock:server": "json-server --watch data/products.json --routes data/routes.json",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^7.2.7",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.7",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^4.0.4",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^7.3.0",
    "@ngrx/entity": "^7.3.0",
    "@ngrx/router-store": "^7.3.0",
    "@ngrx/store": "^7.3.0",
    "@ngrx/store-devtools": "^7.3.0",
    "@ngx-translate/core": "^10.0.2",
    "bootstrap": "^4.1.1",
    "core-js": "^2.6.5",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.11",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.4",
    "@angular/cli": "~7.3.4",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "^7.2.7",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@ngx-rocket/scripts": "^3.0.1",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.10.4",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.0",
    "hads": "^1.7.2",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^2.2.1",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-server": "^0.14.2",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.4.0",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.0",
    "ts-node": "~8.0.2",
    "tslint": "~5.13.1",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.1.1"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
